{
  "name": "cadmus-navbar-admin",
  "version": "1.0.0",
  "description": "A React and Redux-based admin interface for cadmus_navbar",
  "main": "lib/index.js",
  "repository": "https://github.com/nbudin/cadmus-navbar-admin",
  "author": "Nat Budin <nbudin@patientslikeme.com>",
  "license": "MIT",
  "files": [
    "dist/index.*",
    "lib",
    "src"
  ],
  "scripts": {
    "test": "npm run lint",
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "babel src --out-dir lib --copy-files",
    "build:dist": "webpack --config webpack.config.js --progress",
    "prepare": "npm run build",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint:fix": "eslint --ext .js --ext .jsx src --fix",
    "start": "NODE_ENV=development webpack-dev-server",
    "webpack:analyze": "webpack --config webpack.config.js --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "react": "^15.6.1",
    "react-build-dist": "^0.0.8",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-redux": "^5.0.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "json-fetch": "^7.2.0",
    "prop-types": "^15.5.10",
    "react-bootstrap4-modal": "^1.1.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dnd-multi-backend": "^2.3.7",
    "react-html-id": "^0.1.1",
    "react-immutable-proptypes": "^2.1.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
