{
  "name": "cadmus-navbar-admin",
  "version": "4.1.0",
  "description": "A React and Redux-based admin interface for cadmus_navbar",
  "main": "lib/index.js",
  "repository": "https://github.com/nbudin/cadmus-navbar-admin",
  "author": "Nat Budin <natbudin@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/index.*",
    "lib",
    "src"
  ],
  "scripts": {
    "test": "npm run lint",
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "babel src --out-dir lib --copy-files",
    "build:dist": "webpack --config webpack.config.js --progress",
    "prepare": "npm run build",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint:fix": "eslint --ext .js --ext .jsx src --fix",
    "start": "NODE_ENV=development webpack-dev-server",
    "webpack:analyze": "webpack --config webpack.config.js --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/babel__core": "^7",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^2.0.0",
    "core-js": "^3.17.3",
    "css-loader": "^0.28.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "prettier": "^2.4.0",
    "react": "^16.8.0",
    "react-build-dist": "^0.0.8",
    "react-dom": "^16.8.0",
    "style-loader": "^0.18.2",
    "typescript": "^4.4.3",
    "webpack": "^4.0.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.5.10",
    "react-bootstrap4-modal": "^1.2.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-multi-backend": "^6.0.2",
    "react-dnd-touch-backend": "^11.1.3",
    "unfetch": "^4.1.0"
  }
}
