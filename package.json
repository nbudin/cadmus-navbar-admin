{
  "name": "cadmus-navbar-admin",
  "version": "5.0.0-alpha.4",
  "description": "A React-based admin interface for cadmus_navbar",
  "main": "lib/index.js",
  "repository": "https://github.com/nbudin/cadmus-navbar-admin",
  "author": "Nat Budin <natbudin@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/index.*",
    "lib",
    "styles"
  ],
  "scripts": {
    "test": "npm run lint",
    "build": "npm run build:lib && npm run build:dist",
    "build:dist": "rm -rf dist/* && tsc --outDir dist/ --module commonjs",
    "build:lib": "rm -rf lib/* && tsc --outDir lib/",
    "prepare": "npm run build",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint:fix": "eslint --ext .js --ext .jsx src --fix",
    "start": "NODE_ENV=development webpack-dev-server",
    "webpack:analyze": "webpack --config webpack.config.js --json | webpack-bundle-size-analyzer"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/babel__core": "^7.1.16",
    "@types/lodash": "^4.14.172",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bootstrap-icons": "^1.5.0",
    "compression-webpack-plugin": "^9.0.0",
    "core-js": "^3.17.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-build-dist": "^0.0.8",
    "react-dom": "^17.0.2",
    "style-loader": "^3.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/sortable": "^4.0.0",
    "@dnd-kit/utilities": "^2.0.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-bootstrap4-modal": "^2.0.0"
  }
}
